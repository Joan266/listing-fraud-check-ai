# .env.example

# --- General Settings ---
PROJECT_NAME="FraudCheck.ai MVP"
API_V1_STR="/api/v1"

# --- Database Settings ---
# For local SQLite development, use a relative path like this:
DATABASE_URL="sqlite:///./fraudcheck.db"
# For PostgreSQL, the format would be:
# DATABASE_URL="postgresql://user:password@hostname:5432/dbname"


# --- Redis Settings ---
REDIS_HOST="localhost"
REDIS_PORT=6379

# --- Google Cloud API Keys ---
# This key is for Google Maps Platform (Geocoding, Places)
GOOGLE_API_KEY="YOUR_Maps_API_KEY_HERE"